@using Votor.Areas.Portal.Controllers
@model Controllers.EventListModel;

@{
    ViewBag.Title = Localizer["Dashboard"];
}

<div class="row no-margin-bottom">
    <div class="col s12 m6 l4">
        @{ await Html.RenderPartialAsync("_EventChartPartial", Model);}
    </div>

    <div class="col s12 m6 l4">
        @{ await Html.RenderPartialAsync("_CreateEventPartial", Model.CloneEventModel); }
    </div>
</div>
<div class="row no-margin-bottom">
    @{ await Html.RenderPartialAsync("_ShowEventCards", Model.Active);}
</div>